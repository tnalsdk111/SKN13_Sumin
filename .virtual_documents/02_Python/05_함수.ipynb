def calculate(num1, num2):
    r1 = num1 + num2
    r2 = num1 - num2
    r3 = num1 * num2
    r4 = num1 / num2
    # 모든 계산 결과를 반환
    # return [r1, r2, r3, r4]
    # return r1, r2, r3, r4 # 괄호 생략하고 튜플로 리턴
    return dict(plus=r1, minus=r2, multiply=r3, divide=r4)


x = calculate(3, 5)
x


def divide(n1, n2):
    if n2 == 0:
        return    # 함수 실행을 종료하고 호출한 곳으로 돌아가세요
    return n1/n2


result = divide(10, 2)
print(result)

if result is None:
    print("계산 실패")
else:
    print(f"결과: {result}")


a = print("안녕")
print(a) # None # print()는 None을 리턴











